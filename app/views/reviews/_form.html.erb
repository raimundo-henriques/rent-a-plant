<%= simple_form_for [plant, review], data: {insert_in_list_target: "form", action: "submit->insert-in-list#send"} do |f| %>
<div class="d-flex justify-content-center">
  <div class="col-12">
    <%= f.input :rating, collection: (1..5) %>
    <%= f.input :comment %>
  </div>
</div>
  <div class="d-flex justify-content-center">
    <%= f.submit class:'btn btn-success my-5' %>
  </div>
<% end %>
